{
	"editor.bracketPairColorization.enabled": true,
	"editor.cursorBlinking": "solid",
	"editor.cursorSurroundingLines": 99999999,
	"editor.fontFamily": "'Monaspace Neon Light', 'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
	"editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss06', 'ss07', 'ss08', 'calt'",
	"editor.fontSize": 18,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.insertSpaces": false,
	"editor.minimap.enabled": false,
	"editor.renderWhitespace": "trailing",
	"editor.semanticTokenColorCustomizations": {
		"[One Dark Pro]": {
			"enabled": true,
			"rules": {
				"variable.local": "#ABB2BF",
				"parameter": "#F3AEAE"
			}
		},
		"[One Dark Pro Darker]": {
			"enabled": true,
			"rules": {
				"variable.local": "#ABB2BF",
				"parameter": "#F3AEAE"
			}
		}
	},
	"editor.suggestSelection": "first",
	"workbench.colorTheme": "One Dark Pro",
	"oneDarkPro.editorTheme": "One Dark Pro Darker",
	"files.insertFinalNewline": true,
	"files.maxMemoryForLargeFilesMB": 1024,
	"go.useLanguageServer": true,
	"gopls": {
		"formatting.gofumpt": true,
		"ui.semanticTokens": true
	},
	"outline.showArrays": false,
	"outline.showBooleans": false,
	"outline.showFields": false,
	"outline.showFiles": false,
	"outline.showNull": false,
	"outline.showNumbers": false,
	"outline.showObjects": false,
	"outline.showOperators": false,
	"outline.showStrings": false,
	"outline.showStructs": false,
	"outline.showTypeParameters": false,
	"outline.showVariables": false,
	"python.formatting.provider": "black",
	"python.linting.enabled": true,
	"python.linting.flake8Args": [
		"--ignore=E501"
	],
	"python.linting.flake8Enabled": true,
	"rust-client.engine": "rust-analyzer",
	"telemetry.enableCrashReporter": false,
	"telemetry.enableTelemetry": false,
	"telemetry.telemetryLevel": "off",
	"vim.easymotion": true,
	"vim.easymotionKeys": "abcdefghijklmnopqrstuvwxyz0123456789",
	"vim.enableNeovim": true,
	"vim.leader": ",",
	"workbench.startupEditor": "newUntitledFile",
	"window.zoomLevel": 1,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"extensions.experimental.affinity": {
		"vscodevim.vim": 1
	},
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<tab>"
			],
			"after": [
				">",
				"g",
				"v"
			]
		},
		{
			"before": [
				"<s-tab>"
			],
			"after": [
				"<",
				"g",
				"v"
			]
		},
		{
			"before": [
				"<leader>",
				"y"
			],
			"after": [
				"\"",
				"+",
				"y"
			]
		},
		{
			"before": [
				"<leader>",
				"p"
			],
			"after": [
				"\"",
				"+",
				"p"
			]
		},
		{
			"before": [
				"p"
			],
			"after": [
				"P"
			]
		},
		{
			"before": [
				"|"
			],
			"after": [
				":",
				"s",
				"o",
				"r",
				"t",
				"<space>",
				"i",
				"<cr>"
			],
			"comment": "Sort selection"
		},
		{
			"before": [
				"<leader>",
				"c"
			],
			"commands": [
				"editor.action.commentLine"
			]
		},
		{
			"before": ["<leader>", "a", "i"],
			"commands": ["inlineChat.start"]
		},
		{
			"before": ["<leader>", "a", "f"],
			"commands": ["github.copilot.chat.fix"]
		},
		{
			"before": ["<leader>", "a", "e"],
			"commands": ["github.copilot.chat.explain.palette"]
		},
		{
			"before": ["<leader>", "a", "r"],
			"commands": ["github.copilot.chat.review"]
		},
		{
			"before": ["<leader>", "a", "d"],
			"commands": ["github.copilot.chat.generateDocs"]
		},
		{
			"before": ["<leader>", "a", "t"],
			"commands": ["github.copilot.chat.generateTests"]
		}
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"´"
			],
			"after": [
				"`"
			],
			"comment": "Friendly jumping marks for international keyboards"
		},
		{
			"before": [
				"´",
				"´"
			],
			"after": [
				"`",
				"`"
			],
			"comment": "Friendly jumping marks for international keyboards"
		},
		{
			"before": [
				"!"
			],
			"after": [
				":",
				"!"
			],
			"comment": "Quick command execution"
		},
		{
			"before": [
				"H"
			],
			"after": [
				"0"
			],
			"comment": "Jump to start of the line"
		},
		{
			"before": [
				"L"
			],
			"after": [
				"$"
			],
			"comment": "Jump to end of the line"
		},
		{
			"before": [
				"*"
			],
			"after": [
				"m",
				"`",
				"*",
				"`",
				"`"
			],
			"comment": "Jump to end of the line"
		},
		{
			"before": [
				"<space>"
			],
			"after": [
				"<leader>",
				"<leader>"
			],
			"comment": "Replace easymotion activation prefix"
		},
		{
			"before": [
				"<space>",
				"<leader>"
			],
			"after": [
				"<leader>",
				"<leader>",
				"/"
			],
			"comment": "Activate easymotion multi-input search"
		},
		{
			"before": [
				"Q"
			],
			"after": [
				"@",
				"q"
			],
			"comment": "Quick macro execution"
		},
		{
			"before": [
				"x"
			],
			"after": [
				"\"",
				"_",
				"x"
			],
			"comment": "Do not overwrite yank when deleting a character"
		},
		{
			"before": [
				"<leader>",
				"p"
			],
			"after": [
				"\"",
				"+",
				"p"
			],
			"comment": "Paste from system clipboard"
		},
		{
			"before": [
				"<leader>",
				"y"
			],
			"after": [
				"\"",
				"+",
				"y"
			],
			"comment": "Yank into system clipboard"
		},
		{
			"before": [
				"Y"
			],
			"after": [
				"y",
				"$"
			],
			"comment": "Yank till the end of the line"
		},
		{
			"before": [
				"u"
			],
			"commands": [
				"undo"
			]
		},
		{
			"before": [
				"C-r"
			],
			"commands": [
				"redo"
			]
		},
		{
			"before": [
				"<cr>"
			],
			"commands": [
				"workbench.action.files.save"
			]
		},
		{
			"before": [
				"<leader>",
				"d",
				"f"
			],
			"commands": [
				"workbench.action.quickOpen"
			]
		},
		{
			"before": [
				"<leader>",
				"d",
				"d"
			],
			"commands": [
				"workbench.action.findInFiles"
			]
		},
		{
			"before": [
				"<leader>",
				"d",
				"o"
			],
			"commands": [
				"workbench.action.gotoSymbol"
			]
		},
		{
			"before": [
				"s"
			],
			"after": [
				":",
				"%",
				"s",
				"/",
				"/",
				"g",
				"<left>",
				"<left>"
			],
			"comment": "Simpified substitution"
		},
		{
			"before": [
				"S"
			],
			"after": [
				"m",
				"`",
				"\"",
				"_",
				"d",
				"i",
				"w",
				"\"",
				"0",
				"P",
				"`",
				"`"
			],
			"comment": "Substitute word under curser with yank buffer"
		},
		{
			"before": [
				"<leader>",
				"<leader>"
			],
			"commands": [
				"workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
			]
		},
		{
			"before": [
				"<backspace>"
			],
			"commands": [
				"workbench.action.closeActiveEditor"
			]
		},
		{
			"before": [
				"<tab>"
			],
			"commands": [
				"workbench.action.nextEditor"
			]
		},
		{
			"before": [
				"<s-tab>"
			],
			"commands": [
				"workbench.action.previousEditor"
			]
		},
		{
			"before": [
				"#"
			],
			"commands": [
				"workbench.action.quickOpenPreviousRecentlyUsedEditor",
				"workbench.action.acceptSelectedQuickOpenItem"
			]
		},
		{
			"before": [
				"<leader>",
				"c"
			],
			"commands": [
				"editor.action.commentLine"
			]
		},
		{
			"before": [
				"?"
			],
			"commands": [
				"editor.action.showHover"
			]
		},
		{
			"before": ["<leader>", "a", "f"],
			"commands": ["github.copilot.chat.fix"]
		},
		{
			"before": ["<leader>", "a", "e"],
			"commands": ["github.copilot.chat.explain.palette"]
		},
		{
			"before": ["<leader>", "a", "c"],
			"commands": ["workbench.action.chat.openEditSession"]
		}
	],
	"vim.insertModeKeyBindingsNonRecursive": [],
	"svelte.enable-ts-plugin": true,
	"workbench.editor.limit.enabled": true,
	"workbench.editor.limit.value": 8
}
