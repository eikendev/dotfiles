# Show pseudocode in disassembly.
e asm.pseudo = false

# Use Intel syntax.
e asm.syntax = intel

# Use a wide color palette.
e scr.color = 3

# Set theme.
eco solarized

# Use fancy Unicode elements.
scr.utf8.curvy = true

# Resolve relative addresses.
e asm.sub.rel = true

# Do not show fortunes on start.
e cfg.fortunes = false
